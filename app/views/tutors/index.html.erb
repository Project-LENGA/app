<h1>Tutors list</h1>

<table style="width: 100%; border: 1px solid;">
  <thead>
    <tr>
      <td>name</td>
      <td>email</td>
    </tr>
  </thead>

  <tbody>
    <% @tutors.each do |t| %>
      <tr>
        <td> <%= t.name %> </td>
        <td> <%= t.email %> </td>
      </tr>
    <% end %>
  </tbody>
</table>
